# double precision tridiagonl eigensolver using QR
Signature(
    "dsteqr_",
    Flag("compz", ["V", "N"]),
    Dim("n")
    dData("D", "n"),
    dData("E", "n - 1"),
    dData("Z", "ldZ * n if compz == 'V' else 0"),
    Ld("ldZ", "n if compz == 'V' else 1")
    dData("Work", "2 * n - 2 if jobz == 'V' else 0"),
    Info()
)
