# single precision triangular linear system solve with a matrix
Signature(
    "strsm_",
    Side(),
    Uplo(),
    Trans("transA"),
    Diag(),
    Dim("m"),
    Dim("n"),
    sScalar(),
    sData("A", "ldA * (m if side == 'L' else n)",
          "lower if uplo == 'L' else upper"),
    Ld("ldA", "m if side == 'L' else n"),
    sData("B", "ldB * n"),
    Ld("ldB", "m"),
    complexity="m * n * (m if side == 'L' else n + 1)"
)
