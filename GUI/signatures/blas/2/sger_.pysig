# single precision vector-vector outer product
Signature(
    "sger_",
    Dim("m"),
    Dim("n"),
    sScalar(),
    sData("X", "1 + (m - 1) * abs(incX)"),
    Inc("IncX"),
    sData("Y", "1 + (n - 1) * abs(incY)"),
    Inc("IncY"),
    sData("A", "ldA * n"),
    Ld("LdA", "m"),
    complexity = "2 * m * n"
)
