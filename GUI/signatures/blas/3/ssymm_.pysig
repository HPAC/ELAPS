# single precision symmetrix matrix-matrix product
Signature(
    "ssymm_",
    Side(),
    Uplo(),
    Dim("m"),
    Dim("n"),
    sScalar(),
    sData("A", "ldA * (m if side == 'L' else n)",
          "symm, lower if uplo == 'L' else upper"),
    Ld("ldA", "m if side == 'L' else n"),
    sData("B", "ldB * n"),
    Ld("ldB", "m"),
    sScalar("beta"),
    sData("C", "ldC * n"),
    Ld("ldC", "m"),
)
