# double precision symmetrix matrix-matrix product
Signature(
    "dsymm_",
    Side(),
    Uplo(),
    Dim("m"),
    Dim("n"),
    dScalar(),
    dData("A", "ldA * (m if side == 'L' else n)",
          "symm, lower if uplo == 'L' else upper"),
    Ld("ldA", "m if side == 'L' else n"),
    dData("B", "ldB * n"),
    Ld("ldB", "m"),
    dScalar("beta"),
    dData("C", "ldC * n"),
    Ld("ldC", "m"),
    complexity="2 * m * n * (m if side == 'L' else n)"
)
