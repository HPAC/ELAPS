# double precision complex hermetian matrix-vector product
Signature(
    "chemv",
    Uplo(),
    Dim("n"),
    zScalar(),
    zData("A", "ldA * n", "herm, lower if uplo == 'L' else upper"),
    Ld("ldA", "n"),
    zData("X", "1 + (n - 1) * abs(incX)"),
    Inc("incX"),
    zScalar("beta"),
    zData("Y", "1 + (n - 1) * abs(incY)"),
    Inc("incY"),
    flops="4 * 2 * n ** 2"
)
