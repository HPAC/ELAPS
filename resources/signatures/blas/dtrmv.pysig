# double precision triangular matrix-vector product
Signature(
    "dtrmv",
    Uplo(),
    Trans(),
    Diag(),
    Dim("n"),
    dData("A", "ldA * n", "lower if uplo == 'L' else upper"),
    Ld("ldA", "n"),
    dData("X", "1 + (n - 1) * abs(incX)"),
    Inc("incX"),
    flops="n ** 2 + n"  # checked
)
