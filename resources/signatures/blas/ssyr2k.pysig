# single precision symmetric rank 2 k update
Signature(
    "ssyr2k",
    Uplo(),
    Trans(),
    Dim("n"),
    Dim("k"),
    sScalar(),
    sData("A", "ldA * (k if trans == 'N' else n)"),
    Ld("ldA", "n if trans == 'N' else k"),
    sData("B", "ldB * (k if trans == 'N' else n)"),
    Ld("ldB", "n  if trans == 'N' else k"),
    sScalar("beta"),
    sData("C", "ldC * n",
          "symm, lower if uplo == 'L' else upper"),
    Ld("ldC", "n"),
    complexity="2 * k * n ** 2"
)
