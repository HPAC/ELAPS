# double precision double symmetric vector-vector product
Signature(
    "dsyr2",
    Uplo(),
    Dim("n"),
    dScalar(),
    dData("X", "1 + (n - 1) * abs(incX)"),
    Inc("incX"),
    dData("Y", "1 + (n - 1) * abs(incY)"),
    Inc("incY"),
    dData("A", "ldA * n", "lower if uplo == 'L' else upper"),
    Ld("ldA", "n"),
    complexity = "4 * n ** 2"
)
