# single precision triangular matrix-vector product
Signature(
    "strmm",
    Side(),
    Uplo(),
    Trans("transA"),
    Diag(),
    Dim("m"),
    Dim("n"),
    sScalar(),
    sData("A", "ldA * (m if side == 'L' else n)",
          "lower if uplo == 'L' else upper"),
    Ld("ldA", "m if side == 'L' else n"),
    sData("B", "ldB * n"),
    Ld("ldB", "m"),
    flops="m * n * (m if side == 'L' else n + 1)"  # checked
)
