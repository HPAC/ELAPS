# double precision complex hermetian rank k update
Signature(
    "zherk",
    Uplo(),
    cTrans(),
    Dim("n"),
    Dim("k"),
    zScalar(),
    zData("A", "ldA * (k if trans == 'N' else n)"),
    Ld("ldA", "n if trans == 'N' else k"),
    zScalar("beta"),
    zData("C", "ldC * n",
          "symm, lower if uplo == 'L' else upper"),
    Ld("ldC", "n"),
    complexity="4 * k * n ** 2"
)
