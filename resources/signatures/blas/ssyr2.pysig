# single precision double symmetric vector-vector product
Signature(
    "ssyr2",
    Uplo(),
    Dim("n"),
    sScalar(),
    sData("X", "1 + (n - 1) * abs(incX)"),
    Inc("incX"),
    sData("Y", "1 + (n - 1) * abs(incY)"),
    Inc("incY"),
    sData("A", "ldA * n", "lower if uplo == 'L' else upper"),
    Ld("ldA", "n"),
    complexity = "4 * n ** 2"
)
