# double precision complex symmetrix rank k update
Signature(
    "zsyrk",
    Uplo(),
    Trans(),
    Dim("n"),
    Dim("k"),
    zScalar(),
    zData("A", "ldA * (k if trans == 'N' else n)"),
    Ld("ldA", "n if trans == 'N' else k"),
    zScalar("beta"),
    zData("C", "ldC * n",
          "symm, lower if uplo == 'L' else upper"),
    Ld("ldC", "n"),
    flops="4 * k * n ** 2"
)
