# single precision complex hermetian rank k update
Signature(
    "cherk",
    Uplo(),
    cTrans(),
    Dim("n"),
    Dim("k"),
    cScalar(),
    cData("A", "ldA * (k if trans == 'N' else n)"),
    Ld("ldA", "n if trans == 'N' else k"),
    cScalar("beta"),
    cData("C", "ldC * n",
          "symm, lower if uplo == 'L' else upper"),
    Ld("ldC", "n"),
    complexity="4 * k * n ** 2"
)
