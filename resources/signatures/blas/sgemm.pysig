# single precision matrix-matrix product
Signature(
    "sgemm",
    Trans("transA"),
    Trans("transB"),
    Dim("m"),
    Dim("n"),
    Dim("k"),
    sScalar(),
    sData("A", "ldA * (k if transA == 'N' else m)"),
    Ld("ldA", "m if transA == 'N' else k"),
    sData("B", "ldB * (n if transB == 'N' else k)"),
    Ld("ldB", "k if transB == 'N' else n"),
    sScalar("beta"),
    sData("C", "ldC * n"),
    Ld("ldC", "m"),
    complexity = "2 * m * n * k"
)
