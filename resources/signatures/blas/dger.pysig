# double precision vector-vector outer product
Signature(
    "dger",
    Dim("m"),
    Dim("n"),
    dScalar(),
    dData("X", "1 + (m - 1) * abs(incX)"),
    Inc("incX"),
    dData("Y", "1 + (n - 1) * abs(incY)"),
    Inc("incY"),
    dData("A", "ldA * n"),
    Ld("ldA", "m"),
    complexity = "2 * m * n"
)
