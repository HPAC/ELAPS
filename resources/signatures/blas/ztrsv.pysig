# double single precision complex traiangular matrix linear system solve product
Signature(
    "ztrsv",
    Uplo(),
    cTrans(),
    Diag(),
    Dim("n"),
    zData("A", "ldA * n", "lower if uplo == 'L' else upper"),
    Ld("ldA", "n"),
    zData("X", "1 + (n - 1) * abs(incX)"),
    Inc("incX"),
    flops="4 * n * (n + 1)"
)
