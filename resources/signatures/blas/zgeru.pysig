# double precision complex vector-vector outer product
Signature(
    "zgeru",
    Dim("m"),
    Dim("n"),
    zScalar(),
    zData("X", "1 + (m - 1) * abs(incX)"),
    Inc("incX"),
    zData("Y", "1 + (n - 1) * abs(incY)"),
    Inc("incY"),
    zData("A", "ldA * n"),
    Ld("ldA", "m"),
    flops="4 * 2 * m * n"
)
