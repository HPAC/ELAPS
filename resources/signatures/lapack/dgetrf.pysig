# double precision LU factorization with pivoting
Signature(
    "dgetrf",
    Dim("m"),
    Dim("n"),
    dData("A", "ldA * n"),
    Ld("ldA", "m"),
    iData("ipiv", "min(m, n)"),
    Info(),
    complexity="min(m, n) * (min(m, n) * (4 * min(m, n) - 6 * m - 6 * n + 3) + 6 * (2 * m - 1) * n - 1) / 6"
)
