# double precision symmetric reduction to tridiagonal
Signature(
    "dsytrd_",
    Uplo(),
    Dim("n"),
    dData("A", "ldA * n",
          "lower if uplo == 'L' else upper"),
    Ld("ldA", "n"),
    dData("D", "n"),
    dData("E", "n - 1"),
    dData("tau", "n - 1"),
    dData("Work", "lWork"),
    Dim("lWork", 1),
    Info()
)
