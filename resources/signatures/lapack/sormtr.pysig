# single precision mutliplication by Q from ssytrd
Signature(
    "sormtr",
    Side(),
    Uplo(),
    Dim("m"),
    Dim("n"),
    sData("A", "ldA * (m if side == 'L' else n)",
          "lower if uplo == 'L' else upper"),
    Ld("ldA", "m if side == 'L' else n"),
    sData("Tau", "m - 1 if side == 'L' else n - 1"),
    sData("C", "ldC * n"),
    Ld("ldC", "m"),
    sWork("Work", "lWork"),
    Lwork("lWork", "n if side == 'L' else m")
)
