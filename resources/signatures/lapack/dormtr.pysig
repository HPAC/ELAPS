# double precision mutliplication by Q from dsytrd
Signature(
    "dormtr",
    Side(),
    Uplo(),
    Dim("m"),
    Dim("n"),
    dData("A", "ldA * (m if side == 'L' else n)",
          "lower if uplo == 'L' else upper"),
    Ld("ldA", "m if side == 'L' else n"),
    dData("Tau", "m if side == 'L' else n"),
    dData("C", "ldC * n"),
    Ld("ldC", "m"),
    dWork("Work", "lWork"),
    Lwork("lWork", "n if side == 'L' else m")
)
