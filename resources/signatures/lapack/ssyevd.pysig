# single precision symmetirc eigensolver using DC
Signature(
    "ssyevd",
    Flag("jobz", ["V", "N"]),
    Uplo(),
    Dim("n"),
    sData("A", "ldA * n", "symm, lower if uplo == 'L' else upper"),
    Ld("ldA", "n"),
    sData("W", "n"),
    sWork("Work", "lWork"),
    Lwork("lWork", "2 * n + 1 if jobz == 'N' else 1 + 6 * n + 2 * n * n"),
    iWork("iWork", "liWork"),
    Lwork("liWork", "1 if jobz == 'N' else 3 + 5 * n"),
    Info()
)
