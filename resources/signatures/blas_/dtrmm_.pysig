# double precision triangular matrix-vector product
Signature(
    "dtrmm_",
    Side(),
    Uplo(),
    Trans("transA"),
    Diag(),
    Dim("m"),
    Dim("n"),
    dScalar(),
    dData("A", "ldA * (m if side == 'L' else n)",
          "lower if uplo == 'L' else upper"),
    Ld("ldA", "m if side == 'L' else n"),
    dData("B", "ldB * n"),
    Ld("ldB", "m"),
    complexity="m * n * (m if side == 'L' else n + 1)"  # checked
)
